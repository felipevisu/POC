directive @key(fields: String!) on OBJECT | INTERFACE

type Book @key(fields: "id") {
  id: ID!
  title: String
  author: String
  year: Int
  isbn: String
}

type Query {
  books: [Book]
  bookById(id: ID!): Book
  booksByAuthor(author: String!): [Book]
  _entities(representations: [_Any!]!): [_Entity]
  _service: _Service!
}

union _Entity = Book

type _Service {
  sdl: String
}

scalar _Any
